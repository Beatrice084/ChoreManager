<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.beajo.choremanager2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityChoreList">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShoppingActivity" />
        <activity android:name=".ToolsActivity" />
        <activity android:name=".CupboardFridgeActivity" />
        <activity android:name=".OtherActivity" />
        <activity android:name=".PeopleListActivity" />
        <activity android:name=".SwitchUserActivity" />
        <activity android:name=".TaskIndividualActivity" />
        <activity android:name=".PersonIndividualActivity"></activity>
    </application>

</manifest>